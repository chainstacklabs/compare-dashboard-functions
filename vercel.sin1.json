      "maxDuration": 59
    }
  },
  "crons": [
    {
      "path": "/api/read/ethereum",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/read/solana",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/read/ton",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/read/bnbsc",
      "schedule": "*/3 * * * *"
    },
    {
      "path": "/api/read/monad",
      "schedule": "*/3 * * * *"
    },
    {
