      "maxDuration": 59
    },
    "api/read/solana.py": {
      "memory": 300,
      "maxDuration": 59
    },
    "api/read/ton.py": {
      "memory": 200,
      "maxDuration": 59
    },
    "api/read/bnbsc.py": {
      "memory": 400,
      "maxDuration": 59
    },
    "api/read/monad.py": {
      "memory": 400,
      "maxDuration": 59
    },
